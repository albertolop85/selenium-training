<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="Integration Tests">
	<parameter name="url" value="${url}"/>
	<parameter name="port" value="${port}"/>
	<test name="Tests">
		<groups>
			<run>
				<include name="integration" />
			</run>
		</groups>
		<packages>
			<package name="com.dou.meetup" />
		</packages>
	</test>
</suite>
